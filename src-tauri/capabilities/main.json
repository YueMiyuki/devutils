{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "dialog:default",
    "core:window:allow-close",
    "core:window:allow-destroy",
    "core:app:allow-app-hide",
    "core:app:allow-app-show",
    "core:app:default",
    "process:default",
    "process:allow-exit"
  ]
}
