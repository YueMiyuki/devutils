    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.454.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-i18next": "^16.5.1",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.3.1",
    "zustand": "5.0.9"
  },
  "devDependencies": {
